class Solution {
  public:
    long long solve(vector<int>& bt) {
        // code here
        sort(bt.begin(),bt.end());
        
         long long  sum =0;
         long long  total =0;
         long long n = bt.size();
        for( long long  i=0;i<n-1;i++){   //tc:0(nlogn)  // sc:0(1)
              sum+=bt[i];
              total+=sum;
        }
        
        return (long long ) total/n;

    }
};
